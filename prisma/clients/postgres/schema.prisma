generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["tracing"]
  output          = "../clients/postgres"
}

datasource db {
  provider = "postgresql"
  // url      = ""
  // url      = env("NEON_WARM_URL")
  // url      = env("NEON_COLD_URL")
  // url      = env("SUPABASE_WARM_URL")
  url      = env("SUPABASE_COLD_URL")
}

model User {
  id String @id @default(cuid())
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["tracing"]
  output          = "../clients/mysql"
}

datasource db {
  provider = "mysql"
  // url      = ""
  // url      = env("PLANETSCALE_WARM_URL")
  url      = env("PLANETSCALE_COLD_URL")
}

model User {
  id String @id @default(cuid())
}

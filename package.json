  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "pnpm turbo schemas",
    "upgrade": "pnpm upgrade -i",
    "schemas": "npx prisma generate --schema prisma/schemas/postgres.prisma && npx prisma generate --schema prisma/schemas/mysql.prisma && npx prisma generate --schema prisma/schemas/application.prisma",
    "push:application": "npx prisma db push --schema prisma/schemas/application.prisma",
    "push:providers": "npx prisma db push --schema prisma/schemas/postgres.prisma && npx prisma db push --schema prisma/schemas/mysql.prisma",
    "migrate:dev": "npx prisma migrate dev --schema ./prisma/schemas/application.prisma"
  },
